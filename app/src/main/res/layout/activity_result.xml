<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.dosshi.isolationpracticeapplication.ResultActivity">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="157dp"
        android:layout_height="156dp"
        android:layout_marginBottom="16dp"
        android:layout_marginStart="24dp"
        android:contentDescription="@string/todo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/dancer_img" />

    <Button
        android:id="@+id/practiceAdvice"
        android:layout_width="170dp"
        android:layout_height="50dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="13dp"
        android:text="@string/accel"
        app:layout_constraintBottom_toTopOf="@+id/titleback"
        app:layout_constraintEnd_toEndOf="parent"
        tools:text="加速度データ" />

    <Button
        android:id="@+id/titleback"
        android:layout_width="170dp"
        android:layout_height="50dp"
        android:layout_marginBottom="33dp"
        android:layout_marginEnd="16dp"
        android:text="@string/backtop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:text="タイトルに戻る" />

</android.support.constraint.ConstraintLayout>
