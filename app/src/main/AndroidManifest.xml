<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dosshi.isolationpracticeapplication">

    <uses-permission android:name="android.permission.VIBRATE" />

    <!-- Required to act as a custom watch face. -->
    <uses-permission android:name="android.permission.WAKE_LOCK" /> <!-- Required for complications to receive complication data and open the provider chooser. -->
    <uses-permission android:name="com.google.android.wearable.permission.RECEIVE_COMPLICATION_DATA" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainReaearchActivity"
            android:screenOrientation="portrait"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SelectPartsActivity"
            android:label="@string/app_select"
            android:screenOrientation="portrait" />
        <activity
            android:name=".PartsDescriptionActivity"
            android:label="@string/app_description"
            android:screenOrientation="portrait" />
        <activity
            android:name=".PracticeActivity"
            android:label="PracticeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ResultActivity"
            android:label="ResultData"
            android:screenOrientation="portrait" />
    </application>

</manifest>